{
  "name": "railroad/response",
  "type": "library",
  "description": "Create response based on laravel-fractal package",
  "prefer-stable": true,
  "require": {
    "php": "~7.1",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "spatie/laravel-fractal": "^5.4",
    "railroad/resora": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "filp/whoops": "~2.1",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Response\\": "src"
    },
    "files": [
        "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Response\\Tests\\": "tests/"
    }
  }
}
