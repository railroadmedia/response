{
  "name": "railroad/response",
  "type": "library",
  "description": "Create response based on laravel-fractal package",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "spatie/laravel-fractal": "^6.0",
    "railroad/resora": "dev-2.0-php-8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "orchestra/testbench": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Response\\": "src"
    },
    "files": [
        "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Response\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
