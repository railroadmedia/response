{
    "name": "railroad/response",
    "type": "library",
    "description": "Create response based on laravel-fractal package",
    "minimum-stability": "dev",
  "prefer-stable": true,
 "require": {
    "php": "~7.1",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
   "spatie/laravel-fractal":"5.3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "filp/whoops":"~2.1"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Response\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Response\\Tests\\": "tests/"
    }
  }
}
